    <material name="MatViz" specular="1" shininess=".1" reflectance="0.5" rgba=".07 .07 .1 1"/>
  </asset>
  <default>
    <joint armature='0.04' damping="1" limited="true"/>
    <geom friction="0 0 0" density="300" margin="0.002" condim="1" contype="0" conaffinity="0"/>
